[
    {	//This is how you define a method with named parameters.
        "method": "sayHello",
        "params": {
            "name": "Peter"
        },
        "returns": "Hello Peter"
    },
    {	//This is how you define a notification which does not take any parameters.
        "notification": "notifyServer",
        "params": null
    },
    {	//This is how you define a method with positional parameters.
        "method": "addNumbers",
        "params": [
            3,
            4
        ],
        "returns": 7
    }
]
